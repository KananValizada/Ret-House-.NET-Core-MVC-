<!-- TESTIMONIAL -->
<section>
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-lg-6 mx-auto">
                <div class="title__head">
                    <h2 class="text-center text-capitalize">
                        Insanlar nə deyir
                    </h2>
                    <p class="text-center text-capitalize">İnsanların fikirləri</p>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="testimonial owl-carousel owl-theme">
           <vc:people-says></vc:people-says>
        </div>
    </div>
</section>
