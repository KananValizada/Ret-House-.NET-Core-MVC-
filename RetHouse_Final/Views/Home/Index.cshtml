    <div class="container">
        <div class="row">
            <div class="col-md-8 col-lg-6 mx-auto">
                <div class="title__head">
                    <h2 class="text-center text-capitalize">
                        Bizim Bloqdan 
                    </h2>
                    <p class="text-center text-capitalize">
                        Dünyanın hər yerindəki daşınmaz əmlak agentliklərini tapın
                    </p>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="row">
            <vc:blog-item></vc:blog-item>
        </div>
    </div>
</section>
<!-- END BLOG -->
