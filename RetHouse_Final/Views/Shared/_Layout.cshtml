                                <a href="https://www.facebook.com/" class="btn btn-social btn-social-o facebook mr-1">
                                    <i class="fa fa-facebook-f"></i>
                                </a>
                                <a href="https://twitter.com/?lang=en" class="btn btn-social btn-social-o twitter mr-1">
                                    <i class="fa fa-twitter"></i>
                                </a>

                                <a href="https://www.linkedin.com/" class="btn btn-social btn-social-o linkedin mr-1">
                                    <i class="fa fa-linkedin"></i>
                                </a>
                                <a href="https://www.instagram.com/accounts/login/?hl=en" class="btn btn-social btn-social-o instagram mr-1">
