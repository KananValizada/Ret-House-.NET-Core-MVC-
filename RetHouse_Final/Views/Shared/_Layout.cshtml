                                <ul class="list-unstyled ">
                                    <li class="list-inline-item">
                                        <a asp-controller="property" asp-action="index"> Əmlaklar </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a asp-controller="agent" asp-action="index"> Agentlər </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a asp-controller="agency" asp-action="index"> Agentliklər </a>
